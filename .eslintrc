{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/display-name": "off"
  },
  "globals": {
    "React": "writable"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  }
}
