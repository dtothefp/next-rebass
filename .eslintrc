{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/display-name": "off",
    "no-unused-vars": 1,
    "no-unreachable": "off",
    "react/prop-types": 1
  },
  "globals": {
    "React": "writable"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  }
}
